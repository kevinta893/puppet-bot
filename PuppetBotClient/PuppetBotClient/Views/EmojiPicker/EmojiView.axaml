<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="100" d:DesignHeight="100"
             Width="100" Height="100"
             x:Class="PuppetBotClient.Views.EmojiPicker.EmojiView">
  <Canvas>
    <Rectangle Name="EmojiBackground" Width="70" Height="70" Fill="Black"/>
    <Image Name="EmojiPreview" Width="70" Height="70" Stretch="Uniform"/>
    <Image Name="GifIndicator" Width="10" Height="10" Source="/Assets/gif-file.png" Stretch="Uniform"></Image>
  </Canvas>
</UserControl>
